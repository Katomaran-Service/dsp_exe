        </Viewbox>
        
        <Viewbox Stretch="Uniform" Grid.Row="1" Grid.Column="1">
            <Button x:Name="slog_but" HorizontalAlignment="Stretch" Height="Auto" MinHeight="100" Margin="100" VerticalAlignment="Stretch" MinWidth="120" Width="Auto" BorderBrush="{x:Null}" RenderTransformOrigin="0.5,0.5" Click="slog_but_Click" >
