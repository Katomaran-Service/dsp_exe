            <TextBlock x:Name="textBlock12"  Margin="20,30" TextAlignment="Center" TextWrapping="Wrap" Text="107" FontWeight="ExtraBold" FontSize="14" Grid.Row="0" Grid.Column="6" />
            <Rectangle x:Name="r108" Fill="#FFF4F4F5"  Margin="20"  Grid.Row="0" Grid.Column="7"/>
            <TextBlock x:Name="textBlock13"  Margin="20,30" TextAlignment="Center" TextWrapping="Wrap" Text="108" FontWeight="ExtraBold" FontSize="14" Grid.Row="0" Grid.Column="7" />
            <Rectangle x:Name="r109" Fill="#FFF4F4F5"  Margin="20"  Grid.Row="1" Grid.Column="0"/>
            <TextBlock x:Name="textBlock14"  Margin="20,30" TextAlignment="Center" TextWrapping="Wrap" Text="109" FontWeight="ExtraBold" FontSize="14" Grid.Row="1" Grid.Column="0" />
            <Rectangle x:Name="r110" Fill="#FFF4F4F5"  Margin="20"  Grid.Row="1" Grid.Column="1"/>
            <TextBlock x:Name="textBlock15"  Margin="20,30" TextAlignment="Center" TextWrapping="Wrap" Text="110" FontWeight="ExtraBold" FontSize="14" Grid.Row="1" Grid.Column="1" />
