            </Button>
        </Viewbox>
        <Viewbox Stretch="Uniform" Grid.Row="0" Grid.Column="0">
            <Label x:Name="label" Content="INVENTORY" HorizontalAlignment="Stretch" MinHeight="44" Height="auto" Margin="150,180,150,10" VerticalAlignment="Stretch" MinWidth="108" Width="auto" FontWeight="ExtraBold" FontSize="20"/>
        </Viewbox>
        <Viewbox Stretch="Uniform" Grid.Row="0" Grid.Column="1">
            <Label x:Name="label_Copy" Content="ISSUE" MinHeight="44" Height="Auto" Margin="170,180,130,10" VerticalAlignment="Stretch" FontWeight="ExtraBold" FontSize="20" MinWidth="108" Width="64" HorizontalAlignment="Stretch"/>
        </Viewbox>
        <Viewbox Stretch="Uniform" Grid.Row="0" Grid.Column="2">
            <Label x:Name="label_Copy1" Content="PURCHASE ORDER" HorizontalAlignment="Stretch" MinHeight="44" Height="Auto" Margin="150,180,150,10" VerticalAlignment="Stretch" MinWidth="138" Width="Auto" FontWeight="ExtraBold" FontSize="20" />
        </Viewbox>
        <Viewbox Stretch="Uniform" Grid.Row="1" Grid.Column="0">
            <Label x:Name="label_Copy2" Content="ENTRY" HorizontalAlignment="Stretch" MinHeight="44" Height="Auto" Margin="170,180,130,10" VerticalAlignment="Stretch" MinWidth="148" Width="Auto" FontWeight="ExtraBold" FontSize="20" />
        </Viewbox>
        <Viewbox Stretch="Uniform" Grid.Row="1" Grid.Column="1">
            <TextBlock x:Name="label_Copy4" Text="LOG" TextAlignment="Center" HorizontalAlignment="Stretch" MinHeight="44" Height="Auto" Margin="130,180,130,10" VerticalAlignment="Stretch" MinWidth="196" Width="Auto" FontWeight="ExtraBold" FontSize="20" />
        </Viewbox>

    </Grid>
