    <Grid Background="#FFF0EDC4">
        <Grid.RowDefinitions>
            <RowDefinition Height="272.5*"/>
            <RowDefinition Height="272.5*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="384*"/>
            <ColumnDefinition Width="384*"/>
            <ColumnDefinition Width="384*"/>
        </Grid.ColumnDefinitions>
        <Viewbox Stretch="Uniform" Margin="10,50" Grid.ColumnSpan="3" Grid.RowSpan="2" >
            <Image x:Name="image" HorizontalAlignment="Stretch" MinHeight="448" Height="Auto" VerticalAlignment="Stretch" MinWidth="1150" Width="Auto" Source="line.png" Margin="0,90,0,0"/>
        </Viewbox>

        <Viewbox Stretch="Uniform" Grid.Row="0" Grid.Column="0">
            <Button x:Name="inventory_but" Content="" HorizontalAlignment="Stretch" MinHeight="100" Height="Auto" Margin="100,100,100,100" VerticalAlignment="Stretch" MinWidth="120" Width="120" BorderBrush="{x:Null}" RenderTransformOrigin="0.5,0.5" Click="inventory_but_Click" >
                <Button.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Button.RenderTransform>
                <Button.Background>
                    <ImageBrush ImageSource="icons8-warehouse-filled-100 (1).png"/>
                </Button.Background>
            </Button>
        </Viewbox>

        <Viewbox Stretch="Uniform"  Grid.Row="0" Grid.Column="1" >
            <Button x:Name="issue_but" Content="" HorizontalAlignment="Stretch" MinHeight="100" Height="Auto" Margin="100" VerticalAlignment="Stretch" MinWidth="120" Width="Auto" BorderBrush="{x:Null}" RenderTransformOrigin="0.5,0.5" Click="issue_but_Click">
                <Button.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Button.RenderTransform>
                <Button.Background>
                    <ImageBrush ImageSource="icons8-exit-filled-100.png"/>
                </Button.Background>
            </Button>
        </Viewbox>
        <Viewbox Stretch="Uniform" Grid.Row="0" Grid.Column="2">
            <Button x:Name="purchase_but" Content="" HorizontalAlignment="Right" MinHeight="100" Height="Auto" Margin="100" VerticalAlignment="Stretch" MinWidth="120" Width="auto" BorderBrush="{x:Null}" RenderTransformOrigin="0.5,0.5" Click="purchase_but_Click" >
                <Button.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Button.RenderTransform>
                <Button.Background>
                    <ImageBrush ImageSource="icons8-purchase-order-filled-100.png"/>
                </Button.Background>
            </Button>
        </Viewbox>
        <Viewbox Stretch="Uniform" Grid.Row="1" Grid.Column="0">
            <Button x:Name="entry_but" Content="" HorizontalAlignment="Stretch" MinHeight="100" Height="auto" Margin="100" VerticalAlignment="Stretch" MinWidth="120" Width="Auto" BorderBrush="{x:Null}" RenderTransformOrigin="0.5,0.5" Click="entry_but_Click" >
                <Button.RenderTransform>
