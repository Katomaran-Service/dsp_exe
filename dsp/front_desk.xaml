    <Grid Background="#FFF0EDC4">
        <Grid.RowDefinitions>
            <RowDefinition Height="272.5*"/>
            <RowDefinition Height="272.5*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="384*"/>
            <ColumnDefinition Width="384*"/>
            <ColumnDefinition Width="384*"/>
        </Grid.ColumnDefinitions>
        <Viewbox Stretch="Uniform" Margin="10,50" Grid.ColumnSpan="3" Grid.RowSpan="2" >
            <Image x:Name="image" HorizontalAlignment="Stretch" MinHeight="448" Height="Auto" VerticalAlignment="Stretch" MinWidth="1150" Width="Auto" Source="line.png" Margin="0,90,0,0"/>
        </Viewbox>

        <Viewbox Stretch="Uniform" Grid.Row="0" Grid.Column="0">
            <Button x:Name="checkin_but" Content="" HorizontalAlignment="Stretch" MinHeight="100" Height="Auto" Margin="100,100,100,100" VerticalAlignment="Stretch" MinWidth="120" Width="120" BorderBrush="{x:Null}" RenderTransformOrigin="0.5,0.5" Click="checkin_but_Click" >
                <Button.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Button.RenderTransform>
                <Button.Background>
                    <ImageBrush ImageSource="icons8-hotel-check-in-filled-100 (1).png"/>
                </Button.Background>
            </Button>
        </Viewbox>
        
        <Viewbox Stretch="Uniform"  Grid.Row="0" Grid.Column="1" >
