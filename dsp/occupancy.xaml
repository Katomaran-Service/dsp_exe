<Page x:Class="dsp.occupancy"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:dsp"
      mc:Ignorable="d" 
      d:DesignHeight="545" d:DesignWidth="1140"
      Height="Auto" Width="auto"
      Title="occupancy" Loaded="Page_Loaded">

    <Grid Background="#FFF0EDC4">
        <Grid.RowDefinitions>
            <RowDefinition MinHeight="80" MaxHeight="90" Height="*"/>
            <RowDefinition MinHeight="230" Height="*"/>
            <RowDefinition MinHeight="230" Height="*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Margin="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition MinWidth="228" Width="*"/>
                <ColumnDefinition MinWidth="228" Width="*"/>
                <ColumnDefinition MinWidth="228" Width="*"/>
                <ColumnDefinition MinWidth="228" Width="*"/>
                <ColumnDefinition MinWidth="228" Width="*"/>
            </Grid.ColumnDefinitions>
            <Rectangle Fill="#FF00E266"  Margin="75,15,75,30"  Grid.Column="0"/>
            <Viewbox Stretch="Uniform" Grid.Column="0" >
                <TextBlock x:Name="textBlock" Margin="50,55,50,10" Foreground="Black"  TextAlignment="Center" TextWrapping="Wrap" Text="VACANT" FontWeight="ExtraBold" FontSize="14"  />
            </Viewbox>
            
            <Rectangle Fill="Red"  Margin="75,15,75,30"  Grid.Column="1"/>
            <Viewbox Stretch="Uniform" Grid.Column="1">
                <TextBlock x:Name="textBlock1"  Margin="50,55,50,10" TextAlignment="Center" TextWrapping="Wrap" Text="OCCUPIED" FontWeight="ExtraBold" FontSize="14"  />
            </Viewbox>
            
            <Rectangle Fill="#FFFABF8F"  Margin="75,15,75,30"  Grid.Column="2"/>
            <Viewbox Stretch="Uniform" Grid.Column="2">
                <TextBlock x:Name="textBlock2"  Margin="50,55,50,10" TextAlignment="Center" TextWrapping="Wrap" Text="DIRTY" FontWeight="ExtraBold" FontSize="14" Grid.Column="2"/>
            </Viewbox>
            
            <Rectangle Fill="Black"  Margin="75,15,75,30"  Grid.Column="3"/>
            <Viewbox Stretch="Uniform" Grid.Column="3">
                <TextBlock x:Name="textBlock3"  Margin="50,55,50,10" TextAlignment="Center" TextWrapping="Wrap" Text="BLOCKED" FontWeight="ExtraBold" FontSize="14" Grid.Column="3"/>
            </Viewbox>
            
            <Rectangle Fill="#FF0070C0"  Margin="75,15,75,30"  Grid.Column="4"/>
            <Viewbox Stretch="Uniform" Grid.Column="4">
                <TextBlock x:Name="textBlock4"  Margin="50,55,50,10" TextAlignment="Center" TextWrapping="Wrap" Text="BOOKED" FontWeight="ExtraBold" FontSize="14" Grid.Column="4"/>
            </Viewbox>
           
            <Button x:Name="backbut" Content="" HorizontalAlignment="Left" Height="51" VerticalAlignment="Top" Width="67" BorderBrush="{x:Null}" Click="backbut_Click" Grid.Column="0">
                <Button.Background>
                    <ImageBrush ImageSource="arrow-left-bold-circle.png"/>
                </Button.Background>
            </Button>
        </Grid>

        <Grid Grid.Row="1" Margin="30" Background="#FFE0DFD0">
