                <RowDefinition MinHeight="1" Height="*"/>
            </Grid.RowDefinitions>
            <Rectangle x:Name="r101" Fill="#FFF4F4F5"  Margin="20"  Grid.Row="0" Grid.Column="0"/>
            <TextBlock x:Name="textBlock6"  Margin="20,30" TextAlignment="Center" TextWrapping="Wrap" Text="101" FontWeight="ExtraBold" FontSize="14" Grid.Row="0" Grid.Column="0" />
            <Rectangle x:Name="r102" Fill="#FFF4F4F5"  Margin="20"  Grid.Row="0" Grid.Column="1"/>
            <TextBlock x:Name="textBlock7"  Margin="20,30" TextAlignment="Center" TextWrapping="Wrap" Text="102" FontWeight="ExtraBold" FontSize="14" Grid.Row="0" Grid.Column="1" />
